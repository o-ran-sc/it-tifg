      "title": "Recent Test Cases",
      "type": "table",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_TIMESCALEDB}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  tr.test_id,\n  tr.dut_name,\n  tg.name AS group_name,\n  tc.name AS case_name,\n  tc.result AS case_result,\n  tc.start_date,\n  tc.stop_date\nFROM\n  test_cases tc\nJOIN\n  test_runs tr ON tc.test_run_id = tr.id\nJOIN\n  test_groups tg ON tc.group_id = tg.id\nORDER BY\n  tc.start_date DESC\nLIMIT 10",
          "refId": "A",
          "sql": {
            "columns": [
